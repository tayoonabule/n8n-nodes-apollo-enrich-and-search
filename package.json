{
	"name": "n8n-nodes-apolloio",
	"version": "0.3.0",
	"description": "Official n8n node providing full Apollo.io API integration",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://drewl.com",
	"author": {
		"name": "Drewl",
		"email": "tayo@drewl.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tayoonabule/n8n-nodes-apollo-enrich-and-search.git"
	},
	"engines": {
		"node": ">=18.10"
	},
	"main": "dist/nodes/Apollo/index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"n8n:test": "cross-env N8N_CUSTOM_EXTENSIONS=. npx n8n"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ApolloApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Apollo/index.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
